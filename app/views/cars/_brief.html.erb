<% #local params: car,

auction = car.auction
model = car.model
accident = car.accidents.first
%>

<ul>
  <li>
      <%= link_to model.name, auction_path(auction) %>
  </li>
  <li>
    车原地: <%= accident.huji_address%>
  </li>
  <li>
    停放地: <%= accident.tingche_address%>
  </li>
  <li>
    初次登记: <%= car.registered_at%> 
  </li>
</ul>

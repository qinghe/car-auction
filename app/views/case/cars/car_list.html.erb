<%= form_tag( :url => case_cars_path() ) do  %>
  流水号：
  车型：
<%end%>
<table class='car_accident'>
  <tr>
    <td> 流水号</td>
    <td> 车型</td>
    <td> 发布机构</td>
    <td> 保险公司负责人</td>
    <td> 华晨负责人</td>
    <td> 处理方</td>
    <td> 发生时效</td>
  </tr>

  <% if @cars.size > 0%>
    <%@cars.each{|car|%>
          <tr>
            <td> <%= car.serial_no %></td>
            <td> <%= car.model_name %></td>
            <td> 保险公司</td>
            <td> 保险公司</td>
            <td> 华晨</td>
            <td> 保险公司处理</td>
            <td> <%= Time.now - car.created_at %></td>
          </tr>
    <%}%>
<%else%>
   <tr><td colspan="7"> 没有符合条件的车辆</td></tr>
<%end%>
</table>
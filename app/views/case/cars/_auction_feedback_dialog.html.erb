<%
#拍卖公示开始时间
#拍卖公示结束时间
#拍卖地点
#拍卖开始时间
#拍卖结束时间
#拍卖形式
#是否流拍
#流拍次数
#接受委托时间
#是否完成过户
#要求过户时间
#实际过户时间
#拍卖成交价
#出价次数
#首款是否支付、首款支付时间  溢价金额 溢价是否支付 溢价支付时间 参拍人数 保全人员 保全时间 保全说明
@auction = @car.auction
%>
<% if @car.auctioning? && @auction.closed? %>

  <div id="auction_feedback_dialog" title="拍卖结果">
    <%= form_for(@car, :url => feedback_auction_case_car_path(@car),:html=>{:id=>'auction_feedback_form'}, :remote=>true ) do |car_form| %>
      <table class='car_accident'>
        <%= car_form.fields_for :auction, @auction do |auction_form| %>
        <%= auction_form.hidden_field :type_name,:value=>'2' %>
        <%= auction_form.hidden_field :location,:value=>'网络' %>
          <tr class="aution_info">
            <td colspan="6" class="table_title">拍卖信息</td>
          </tr>
          <tr class="aution_info">
            <td>拍卖公示开始时间</td>
            <td><%= @auction.public_start_at.to_s(:db) if @auction.public_start_at%></td>
            <td>拍卖公示结束时间</td>
            <td><%= @auction.public_expired_at.to_s(:db) if @auction.public_expired_at %></td>
            <td>接受委托时间</td>
            <td><%= @auction.commissioned_time.to_s(:db) if @auction.commissioned_time %></td>
          </tr>
          <tr class="aution_info">
            <td>拍卖开始时间</td>
            <td><%= @auction.start_at.to_s(:db) %></td>
            <td>拍卖结束时间</td>
            <td><%= @auction.expired_at.to_s(:db) %></td>
            <td>是否流拍/流拍次数</td>
            <td><%= @auction.pass_times %></td>

          </tr>

          <tr class="aution_info">
              <td>拍卖成交价 </td>
              <td><%= @auction.bidding_price %></td>
              <td>出价次数</td>
              <td><%= @auction.offers.count %></td>
              <td>参拍人数 </td><td><%= @auction.offerers.count %> </td>
          </tr>

          <tr class="aution_info">
            <td>是否完成过户 </td>
            <td><%= auction_form.select :transfer_complete, [['否','N'],['是','Y']], selected: 'N' %></td>
            <td>要求过户时间</td>
            <td><%= auction_form.text_field :transfer_request_time, class: 'datetimepicker', value: @auction.transfer_request_time.to_s   %></td>
            <td>实际过户时间</td>
            <td><%= auction_form.text_field :transfer_real_time, class: 'datetimepicker', value: @auction.transfer_real_time.to_s  %></td>
          </tr>

          <tr class="aution_info">
            <td>首款是否支付 </td>
            <td><%= auction_form.select :is_pay_first_price, [['否','N'],['是','Y']], selected: 'N' %></td>
            <td>首款支付时间</td>
            <td><%= auction_form.text_field :first_price_pay_date, class: 'datetimepicker', value: @auction.first_price_pay_date.to_s %></td>
            <td>附件组ID</td>
            <td><%= auction_form.text_field :document_group_id, readonly:true %></td>
          </tr>
          <tr class="aution_info">
            <td>溢价是否支付 </td>
            <td><%= auction_form.select :is_pay_premium, [['否','N'],['是','Y']], selected: 'N' %></td>
            <td>溢价支付时间</td>
            <td><%= auction_form.text_field :premium_pay_date, class: 'datetimepicker', value: @auction.premium_pay_date.to_s  %></td>
            <td>溢价金额</td>
            <td><%= auction_form.text_field :premium_price %></td>
          </tr>

          <tr class="aution_info">
            <td>保全人员 </td>
            <td><%= auction_form.text_field :preservation_user %></td>
            <td>保全时间</td>
            <td><%= auction_form.text_field :preservation_date, class: 'datetimepicker', value: @auction.preservation_date.to_s  %></td>
            <td>保全说明</td>
            <td><%= auction_form.text_area :preservation_desc %></td>
          </tr>
          <tr class="aution_info">
            <td> 拍卖结果反馈</td>
            <td colspan="5">
              <div id="car_auction_image_list">
                <%= render :partial=>'car_file', :collection=>@car.bid_images%>
              </div>
              <div class="clear">
                <%= render :partial=>"upload_file", :locals=>{:object=>@car.bid_images.build, :field_name=>'uploaded'}%>
              </div>
            </td>
          </tr>

        <%end%>

      </table>
    <%end%>
  </div>

<%= javascript_tag do %>
      $(function() {
        $( "#auction_feedback_dialog" ).dialog({
          autoOpen: false,
          width:800,
          modal: true,
          buttons: {
            "提交": function() {
               $( "#auction_feedback_form" ).submit();
            },
            "取消": function() {
              $( this ).dialog( "close" );
            }
          },
          close: function() {
          }
        });
      $( "#auction_feedback_button" )
          .click(function() {
            $( "#auction_feedback_dialog" ).dialog( "open" );
          });
      });
  <%end %>
<%end %>
